{
	"name": "deep_logistics",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"astro": "astro",
		"build": "astro build",
		"cl": "powershell -Command \"Remove-Item -LiteralPath node_modules -Recurse -Force; bun install\"",
		"dev": "concurrently \"bun run tailwind:watch\" \"bun -b run dev:astro\"",
		"dev:astro": "bun -b astro dev --host --port",
		"fix": "bunx -b oxlint --fix",
		"format": "prettier --write ./src",
		"format:dist": "prettier --write ./dist/**/*.html",
		"inlang": "bunx -b @inlang/paraglide-js compile --project ./project.inlang --outdir ./src/paraglide && rm -f ./src/paraglide/.gitignore",
		"lint": "prettier --check ./src && concurrently \"bun -b run format\" \"bun -b run lint:oxlint\"",
		"lint:debug": "oxlint ./src --debug",
		"lint:eslint": "bunx -b eslint",
		"lint:oxlint": "bunx -b oxlint",
		"machine-translate": "inlang machine translate --project project.inlang",
		"oxm": "bunx -b @oxlint/migrate",
		"pb": "./pb/pocketbase serve",
		"preview": "astro preview",
		"tailwind:watch": "bunx -b -b @tailwindcss/cli -i ./src/styles/global.css -o ./src/styles/front-output.css --watch",
		"vite:debug": "vite --debug",
		"wc": "concurrently  \"bun -b run wc:lit\" \"bun -b run wc:vue\" \"bun -b run wc:svelte\"",
		"wc:lit": "bun -b run vite build -c vite.config.lit.ts",
		"wc:svelte": "bun -b run vite build -c vite.config.sv.ts",
		"wc:vue": "bun -b run vite build -c vite.config.vue.ts"
	},
	"lint-staged": {
		"**/*.{js,mjs,cjs,jsx,ts,mts,cts,tsx,vue,astro,svelte}": "oxlint"
	},
